{
  "name": "@shuymn/kotap",
  "version": "1.0.1",
  "description": "King of Time auto punching tool",
  "main": "./src/index.js",
  "scripts": {
    "clean": "npm r -g && find ./src -type f -name '*.js' -delete; rm ./tsconfig.tsbuildinfo",
    "build": "npm run build-local && npm i -g --loglevel=error",
    "build-fresh": "npm cache clean --force && npm i && npm run clean && npm run build",
    "build-local": "tsc --project tsconfig.json",
    "watch": "tsc --project tsconfig.json --watch",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "test": "npm run build-local && jest"
  },
  "engines": {
    "node": "16.13.2"
  },
  "bin": {
    "kotap": "./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shuymn/kotap"
  },
  "files": [
    "src/**/*.js"
  ],
  "author": "Shu Yamani",
  "license": "MIT",
  "dependencies": {
    "cli-spinner": "^0.2.10",
    "commander": "^8.3.0",
    "fp-ts": "^2.11.8",
    "is-online": "^9.0.1",
    "luxon": "^2.2.0",
    "puppeteer": "^5.5.0"
  },
  "devDependencies": {
    "@types/cli-spinner": "0.2.1",
    "@types/jest": "27.4.0",
    "@types/luxon": "2.0.8",
    "@types/node": "16.11.21",
    "@types/puppeteer": "5.4.4",
    "@types/rewire": "2.5.28",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "eslint": "8.7.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "rewire": "5.0.0",
    "ts-jest": "27.1.3",
    "ts-node": "10.4.0",
    "typescript": "4.5.5"
  },
  "volta": {
    "node": "16.13.2"
  }
}
