{
  "name": "@shuymn/kotap",
  "version": "1.0.0",
  "description": "King of Time auto punching tool",
  "main": "./src/index.js",
  "scripts": {
    "clean": "npm r -g && find ./src -type f -name '*.js' -delete; rm ./tsconfig.tsbuildinfo",
    "build": "npm run build-local && npm i -g --loglevel=error",
    "build-fresh": "npm cache clean --force && npm i && npm run clean && npm run build",
    "build-local": "tsc --project tsconfig.json",
    "watch": "tsc --project tsconfig.json --watch",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "test": "npm run build-local && jest"
  },
  "engines": {
    "node": "12.19.1"
  },
  "bin": {
    "kotap": "./src/index.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shuymn/kotap"
  },
  "files": [
    "src/**/*.js"
  ],
  "author": "Shu Yamani",
  "license": "MIT",
  "dependencies": {
    "cli-spinner": "^0.2.10",
    "commander": "^5.1.0",
    "fp-ts": "^2.8.6",
    "is-online": "^8.5.1",
    "luxon": "^1.25.0",
    "puppeteer": "^3.3.0",
    "strip-bom": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/cli-spinner": "0.2.0",
    "@types/jest": "25.2.3",
    "@types/luxon": "1.25.0",
    "@types/node": "14.14.9",
    "@types/puppeteer": "3.0.5",
    "@types/rewire": "2.5.28",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "prettier": "2.2.0",
    "rewire": "5.0.0",
    "ts-jest": "26.4.4",
    "ts-node": "8.10.2",
    "typescript": "4.1.2"
  }
}
